#***********************************************************************
# PDU
#***********************************************************************
# power supply status
# powerSupplyOneOk (1), powerSupplyOneFailed (2)
record(snmp, "${A}:${P}_PowerSupply1Status") {
    field(DESC, "Power Supply1 Status")
    field(DTYP, "SNMP Read")
    field(SCAN, "5 second")
    field(VERS, "${V2C}")
    field(HOST, "${HOST}")
    field(COMM, "${COM}")
    field(OIDS, "${PO}rPDUPowerSupply1Status.0")
    field(MDEL, "-1")
}

# outlet8 status
record(snmp, "${A}:${P}_Outlet8_R") {
    field(DESC, "PDU outlet8 control")
    field(DTYP, "SNMP Read")
    field(SCAN, "5 second")
    field(VERS, "${V2C}")
    field(HOST, "${HOST}")
    field(COMM, "${COM}")
    field(OIDS, "${PO}sPDUOutletCtl.8")
}
record(snmp, "${A}:${P}_Outlet8_W") {
    field(DESC, "PDU outlet8 control")
    field(DTYP, "SNMP Write")
    field(SCAN, "5 second")
    field(VERS, "${V3}")
    field(AUTH, "${AUTH_P}")
    field(PRIV, "${PRIV_P}")
    field(HOST, "${HOST}")
    field(COMM, "${USER}")
    field(OIDS, "${PO}sPDUOutletCtl.8")
}

